{
  "resources": {
    "app1": {
      "type": "project.v0",
      "path": "../WebApplication3/WebApplication3.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "app2": {
      "type": "project.v0",
      "path": "../WebApplication3/WebApplication3.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "ingress": {
      "type": "ingress.v0",
      "routes": {
        "app1": {
          "match": {
            "path": "/app1"
          },
          "destination": "app1"
        },
        "app2": {
          "match": {
            "path": "/app2"
          },
          "destination": "app2"
        }
      }
    }
  }
}